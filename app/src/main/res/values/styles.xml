<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/primaryLightColor</item>
    </style>

    <style name="style1">
        <item name="android:colorBackground">#F00</item>
        <item name="android:strokeColor">#F00</item>
        <item name="android:strokeWidth">10</item>
    </style>

    <style name="style2">
        <item name="android:textColor">#40008F</item>
    </style>

    <style name="style3">
        <item name="android:textColor">#033D4B</item>
        <item name="android:colorBackground">#F00</item>
    </style>

    <style name="NewTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/primaryLightColor</item>
        <item name="android:textColorPrimary">@color/primaryTextColor</item>
    </style>
</resources>
